<style>
  .center::selection {
    background: #3e363f; /* Dark background */
    color: white; /* White text */
  }
  .center::-moz-selection {
    background: #3e363f; /* Dark background */
    color: white; /* White text */
  }
  .light::selection {
    background: #3e363f; /* Dark background */
    color: white; /* White text */
  }
  .light::-moz-selection {
    background: #3e363f; /* Dark background */
    color: white; /* White text */

  }
</style>
<div class="container">
  <div class="section">
    <!--   Icon Section   -->
    <div class="row">
      <% for (var i = 0; i < icons.length; i++) { %>
      <div class="col s12 m4">
        <div class="icon-block">
          <h2 class="center green-text">
            <i class="material-icons black"><%=icons[i].name%></i>
          </h2>
          <h5 class="center"><%=icons[i].title%></h5>

          <p class="light"><%=icons[i].description%></p>
        </div>
      </div>
      <% } %>
    </div>
  </div>
</div>
