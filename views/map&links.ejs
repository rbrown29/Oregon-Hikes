<footer class="page-footer green">
  <div class="container">
    <div class="row">
      <div class="col l6 s12">
        <h5 class="black-text">Oregon Hikes</h5>
        <iframe
          class="alltrails"
          src="https://www.alltrails.com/widget/us/oregon"
          width="100%"
          height="400"
          frameborder="0"
          scrolling="no"
          marginheight="0"
          marginwidth="0"
          title="Trail Guides and Maps for Hiking, Camping, and Running"
        ></iframe>
      </div>
      <div class="col l6 s12">
        <h5 class="black-text">Oregon Backpacking</h5>
        <iframe
          width="100%"
          height="400"
          src="https://www.youtube.com/embed/xPMD351GABI?si=bT6jZ0ZQW04_ZtXr"
          title="YouTube video player"
          frameborder="0"
          allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share"
          allowfullscreen
        ></iframe>
      </div>
      <div class="row">
        <% for (var j = 0; j < names.length; j++) { %>
          <div class="col s12 m4">
            <h5 class="black-text"><%= names[j] %></h5>
            <% if (j === 0) { %>
              <% for (var i = 0; i < 7 && i < links.length; i++) { %>
                <ul>
                  <li>
                    <a class="black-text" target="_blank" href="<%= links[i].src %>">
                      <%= links[i].title %>
                    </a>
                  </li>
                </ul>
              <% } %>
            <% } else if (j === 1) { %>
              <% for (var i = 7; i < 20 && i < links.length; i++) { %>
                <ul>
                  <li>
                    <a class="black-text" target="_blank" href="<%= links[i].src %>">
                      <%= links[i].title %>
                    </a>
                  </li>
                </ul>
              <% } %>
            <% } else if (j === 2) { %>
              <% for (var i = 20; i < 28 && i < links.length; i++) { %>
                <ul>
                  <li>
                    <a class="black-text" target="_blank" href="<%= links[i].src %>">
                      <%= links[i].title %>
                    </a>
                  </li>
                </ul>
              <% } %>
            <% } %>
          </div>
        <% } %>
      </div>
      
    </div>
  </div>
</footer>
